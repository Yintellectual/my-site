<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC  
        "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"  
        "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">


<tiles-definitions>
	

	<definition name="DefaultTemplate" template="/WEB-INF/views/template/SiteTemplate.jsp">
		<put-attribute name="title" value="Home" />
		<put-attribute name="header" value="/WEB-INF/views/template/header.jsp" />
		<put-attribute name="menu" value="/WEB-INF/views/template/menu.jsp" />
		<put-attribute name="body" value="" />
		<put-attribute name="footer" value="/WEB-INF/views/template/footer.jsp" />
	</definition>

	<definition name="NoChange" template="/WEB-INF/views/template/NoChange.jsp">
		<put-attribute name="cssfilecommon" value=""/>
		<put-attribute name="body" value="" />
	</definition>

	<definition name="index" extends="NoChange">
		 <put-attribute name="cssfilecommon" value="/WEB-INF/css/w3css.jsp" />
		<put-attribute name="body" value="/index.jsp" />
	</definition>

	<definition name="welcome" extends="DefaultTemplate">
		<put-attribute name="body" value="/welcome.jsp" />
	</definition>


</tiles-definitions>